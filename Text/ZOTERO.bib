@misc{forsterGradedSemanticsGraded2024,
  title = {Graded {{Semantics}} and {{Graded Logics}} for {{Eilenberg-Moore Coalgebras}}},
  author = {Forster, Jonas and Schr{\"o}der, Lutz and Wild, Paul and Beohar, Harsh and Gurke, Sebastian and Messing, Karla},
  year = {2024},
